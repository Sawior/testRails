<h1 align="middle"> Nasze projekty</h1>

<% @posts.each do |post| %>
	<div class='post' style='position: absolute; left: <%= post.x %>px; top: <%= post.y %>px'>
		<div class='text_title'>
			<%= link_to post.title, post %>
			</div>
		<div>
			<%= post.text.html_safe %>
		</div>
		<div class='post_fotter'>
			Dodano przez: <b><%= post.user.email %></b>, <%= time_ago_in_words post.created_at  %> temu.
		</div>	
	</div>
<% end %>
